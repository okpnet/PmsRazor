@inherits RazorCore
@typeparam TModel where TModel :class   

@switch (Row.Status)
{
    case RowStatus.Edit:
        break;
    default:
        @foreach(var column in Columns)
        {
            <TableRowCellContent Column="column" AdditionalAttributes="AdditionalAttributes" StringValue=""></TableRowCellContent>
        }
        break;
}
