@inherits RazorCore
@typeparam TModel where TModel :class   

<tr @attributes="MergedAttributes">
    @switch (Row.Status)
    {
        case RowStatus.Edit:
            <TableRowEditCellContent Row="Row" Columns="Columns"></TableRowEditCellContent>
            break;
        default:
            @foreach (var column in Columns)
            {
                <TableRowCellContent Column="column" Row="Row"></TableRowCellContent>
            }
            break;
    }
</tr>