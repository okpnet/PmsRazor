@inherits OptionParameterRazorCore
@typeparam TModel where TModel : class
@typeparam TProperty

<div @attributes="MergeAttributes">
    <LabelContent @ref="LabelContentRef" ViewOptionKey="@this.CreatePropertyPathkey(t=>t.LabelContentRef)">
        <ChildContent>
            @ChildContent
        </ChildContent>
    </LabelContent>
    <FieldContent Property="ExpressionProperty" Model="Model" ViewOptionKey="@this.CreatePropertyPathkey(t=>t.FieldContentRef)" FieldDataTypes="DataType" FieldConfigOptionKey="FieldConfigOptionKey"></FieldContent>
</div>